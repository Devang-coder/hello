{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.8",
        "includeFiles": [
          "templates/**",
          "static/**",
          "dysgraphia_model.h5",
          "app.py"
        ]
      }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/.*",
      "dest": "api/index.py"
    }
  ]
}
